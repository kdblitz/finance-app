{
  "name": "finance-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "classnames": "^2.2.5",
    "firebase": "4.3.0",
    "gh-pages": "^1.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.12.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "engine": {
    "node": "*",
    "npm": "*"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --content-base build/ -w",
    "build": "webpack",
    "predeploy": "cross-env PUBLIC_PATH='finance-app/' npm run build && cp -r .circleci/ build/",
    "deploy": "cross-env NODE_DEBUG=gh-pages gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.1.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1"
  },
  "homepage": "https://kdblitz.github.io/finance-app"
}
